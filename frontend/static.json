{
  "root": "./dist",
  "clean_urls": true,
  "routes": {
    "/**": "index.html"
  },
  "proxies": {
    "/api/": {
      "origin": "${API_URL}"
    }
  },
  "headers": {
    "/**": {
      "Cache-Control": "no-store, no-cache",
      "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
      "X-Frame-Options": "DENY"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000"
    }
  }
} 